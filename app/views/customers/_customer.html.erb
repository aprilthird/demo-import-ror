<div id="<%= dom_id customer %>">
  <p>
    <strong>First name:</strong>
    <%= customer.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= customer.last_name %>
  </p>

  <p>
    <strong>Occupation:</strong>
    <%= customer.occupation %>
  </p>

  <p>
    <strong>Company:</strong>
    <%= customer.company %>
  </p>

  <p>
    <strong>Age:</strong>
    <%= customer.age %>
  </p>

  <p>
    <strong>Salary:</strong>
    <%= customer.salary %>
  </p>

  <p>
    <strong>Address:</strong>
    <%= customer.address %>
  </p>

</div>
